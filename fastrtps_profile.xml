<?xml version="1.0" encoding="UTF-8" ?>
<dds>
  <profiles>
    <participant profile_name="evaluator_participant" is_default_profile="true">
      <metatrafficPeriod>100000000</metatrafficPeriod> <!-- 100ms -->
      <rtps>
        <name>evaluator</name>
        <defaultUnicastLocatorList>
          <locator>
            <udpv4>
			    <!-- <address>192.168.1.4</address> -->
            </udpv4>
          </locator>
        </defaultUnicastLocatorList>

        <!-- Multicast -->
        <defaultMulticastLocatorList>
          <locator>
            <udpv4>
		    <!-- <address>239.255.0.1</address> -->
            </udpv4>
          </locator>
        </defaultMulticastLocatorList>

        <!-- Optional -->
	<builtin>
 	   <metatrafficUnicastLocatorList>
            <locator>
              <udpv4>
			      <!-- <address>192.168.1.5</address>-->
              </udpv4>
            </locator>
          </metatrafficUnicastLocatorList>

          <initialPeersList>
            <locator>
              <udpv4>
                <address>192.168.1.5</address>
              </udpv4>
            </locator>
          </initialPeersList>
        </builtin>

      </rtps>
    </participant>
  </profiles>
</dds>

